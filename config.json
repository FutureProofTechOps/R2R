{
  "app": {
    "max_logs": 100,
    "max_file_size_in_mb": 32
  },
  "embedding": {
    "provider": "openai",
    "search_model": "text-embedding-3-small",
    "search_dimension": 512,
    "batch_size": 32,
    "text_splitter": {
      "type": "recursive_character",
      "chunk_size": 512,
      "chunk_overlap": 20
    },
    "rerank_model": "None"
  },  
  "eval": {
    "provider": "parea",
    "sampling_fraction": 1.0
  },
  "ingestion":{
    "selected_ingestors": {
      "csv": "default",
      "docx": "default",
      "html": "default",
      "json": "default",
      "md": "default",
      "pdf": "default",
      "pptx": "default",
      "txt": "default"
    }
  },
  "language_model": {
    "provider": "litellm"
  },
  "logging": {
    "provider": "local",
    "log_table": "logs",
    "log_info_table": "log_info",
    "level": "INFO"
  },
  "prompt": {
    "provider": "local"
  },
  "vector_database": {
    "provider": "local",
    "collection_name": "demo_vecs",
    "db_path": "none"
  }
}